{
  "name": "combodo/itop-client-bundle",
  "description": "iTop rest client bundle for symfony",
  "type": "symfony-bundle",
  "license": "AGPL-3.0-only",

  "authors": [
      {
          "name": "bruno DA SILVA",
          "email": "bruno.dasilva@combodo.com"
      }
  ],

  "require": {
    "php": "^7.1|^8",
    "symfony/framework-bundle": "4.4.*",
    "guzzlehttp/guzzle": "^6.3",
    "mtdowling/jmespath.php": "^2.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^7",
    "squizlabs/php_codesniffer": "^3.3",
    "phpmd/phpmd": "^2.6"
  },
  "conflict": {
    "symfony/symfony": "<=3"
  },
  "autoload": {
    "psr-4": {
      "Combodo\\ItopClientBundle\\":["src/"]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Combodo\\ItopClientBundle\\":[ "Tests/"]
    }
  },
  "extra": {
    "branch-alias": {
      "v2.x": "2.0.1-dev"
    }
  }
}
